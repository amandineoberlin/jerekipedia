{
  "manifest_version": 2,

  "name": "Jerekipedia",
  "description": "This extension load all visited wikipedia pages to system folder",
  "version": "1.0",
  "background": {
    "scripts":["background.js"],
    "persistent": false
  },
  "browser_action": {
   "default_icon": "icon.png"
  },
  "permissions": [
    "tabs",
    "pageCapture",
    "downloads"
  ]
}
